<h3>Games With Benchmarks</h3>
<br>
<table class="table table-bordered">
  <thead class="table-borderless">
    <th bgcolor="#f8931f">Name</th>
    <th bgcolor="#ad6511">Benchmarks</th>
  </thead>
  <tbody>
    <% @games.each do |game| %>
      <% if game.benches.any? %>
        <tr>
            <td><%= link_to game.name, game %></td>
            <td><%= game.benches.count %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>